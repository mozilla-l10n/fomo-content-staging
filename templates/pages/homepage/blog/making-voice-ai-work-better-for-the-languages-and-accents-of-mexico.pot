msgid "WHAT"
msgstr ""

msgctxt "body.4cfff7ec-296c-4797-8354-568e05f99ea7"
msgid "Cash prize hackathon for speech-enabled software in all Mexican languages"
msgstr ""

msgctxt "body.4cfff7ec-296c-4797-8354-568e05f99ea7"
msgid "Panels, demos and workshops on languages of Mexico / issue names"
msgstr ""

msgctxt "body.4cfff7ec-296c-4797-8354-568e05f99ea7"
msgid "Mexican Spanish dataset growth marathon - just bring your voice, no code required!"
msgstr ""

